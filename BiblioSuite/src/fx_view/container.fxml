<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="720" minWidth="1280" xmlns="http://javafx.com/javafx/9.0.1"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Container">
	<top>
		<AnchorPane BorderPane.alignment="CENTER">
			<children>
				<JFXTabPane  fx:id="topbar" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" tabClosingPolicy="UNAVAILABLE" />
				<JFXTabPane fx:id="rightbar" AnchorPane.rightAnchor="10.0"
					tabClosingPolicy="UNAVAILABLE">
					<tabs>
						<Tab fx:id="logout" text="Logout">
							<graphic>
								<ImageView fitHeight="20.0" fitWidth="20.0"
									pickOnBounds="true">
									<image>
										<Image url="@../../resources/favicon/128/close.png" />
									</image>
								</ImageView>
							</graphic>
						</Tab>
					</tabs>
				</JFXTabPane>
			</children>
		</AnchorPane>
	</top>
	<center>
		<BorderPane fx:id="content" layoutY="60" />
	</center>

</BorderPane>
