<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="720" minWidth="1280"
	xmlns="http://javafx.com/javafx/9.0.1"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Container">
	<top>
		<AnchorPane BorderPane.alignment="CENTER">
			<children>
				<TabPane fx:id="topbar" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0" tabClosingPolicy="UNAVAILABLE" />
				<Button fx:id="logout" mnemonicParsing="false" text="Logout"
					AnchorPane.rightAnchor="10.0" layoutY="2.5" />
			</children>
		</AnchorPane>
	</top>
	<center>
				<BorderPane fx:id="content" layoutY="60"/>
	</center>

</BorderPane>
